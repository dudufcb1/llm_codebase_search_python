{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "path": "src/server.py",
    "entrypoint": "mcp"
  },
  "environment": {
    "type": "uv",
    "python": ">=3.10",
    "dependencies": [
      "fastmcp",
      "qdrant-client",
      "httpx",
      "pydantic",
      "pydantic-settings",
      "python-dotenv"
    ]
  },
  "deployment": {
    "transport": "stdio",
    "log_level": "INFO"
  }
}

